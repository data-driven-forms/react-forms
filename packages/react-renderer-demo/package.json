{
    "name": "@data-driven-forms/react-renderer-demo",
    "version": "3.18.0",
    "main": "firebaseFunctions.js",
    "description": "Documentation page for Data Driven Forms.",
    "homepage": "https://data-driven-forms.org/",
    "bugs": "https://github.com/data-driven-forms/react-forms/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/data-driven-forms/react-forms",
        "directory": "packages/react-renderer-demo"
    },
    "engines": {
        "node": "16"
    },
    "private": true,
    "scripts": {
        "dev": "next src/",
        "build": "yarn generate:examples && yarn next build src/",
        "start": "next start src/",
        "serve": "cross-env NODE_ENV=production firebase emulators:start --only functions,hosting",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "NODE_ENV=production yarn build && yarn firebase deploy --only functions,hosting",
        "logs": "firebase functions:log",
        "analyze": "ANALYZE=true next build src/",
        "lint": "eslint ./src",
        "generate:examples": "node ./scripts/generate-component-examples && yarn lint --fix"
    },
    "dependencies": {
        "@actions/github": "^5.0.0",
        "@data-driven-forms/form-builder": "0.0.13-rc1",
        "@data-driven-forms/mui-component-mapper": "*",
        "@data-driven-forms/editor-pro": "0.0.10",
        "@data-driven-forms/react-form-renderer": "*",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mdx-js/loader": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@mui/icons-material": "^5.2.5",
        "@mui/material": "^5.2.8",
        "@mui/styles": "^5.2.3",
        "@next/bundle-analyzer": "^12.1.0",
        "@next/mdx": "^12.0.1",
        "@rvsia/next-offline": "^5.0.5",
        "@stackblitz/sdk": "^1.5.3",
        "clsx": "^1.1.1",
        "codesandbox": "2.2.3",
        "docsearch.js": "^2.6.3",
        "evergreen-ui": "^6.9.10",
        "firebase-admin": "^10.0.1",
        "firebase-functions": "^3.16.0",
        "markdown-to-jsx": "^7.1.6",
        "next": "12.1.0",
        "prism-react-renderer": "^1.2.1",
        "prop-types": "^15.8.1",
        "raw-loader": "^4.0.2",
        "react": "^17.0.2",
        "react-copy-to-clipboard": "^5.0.4",
        "react-dom": "^17.0.2",
        "react-github-btn": "^1.2.2",
        "react-sticky-box": "^1.0.2",
        "webpack": "^5.66.0"
    },
    "devDependencies": {
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "firebase-functions-test": "^0.3.3",
        "firebase-tools": "^10.1.0",
        "rimraf": "^3.0.2"
    }
