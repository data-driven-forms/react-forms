{
    "name": "@data-driven-forms/react-renderer-demo",
    "version": "3.7.0",
    "main": "firebaseFunctions.js",
    "description": "Documentation pages for data driven forms.",
    "engines": {
        "node": "10"
    },
    "private": true,
    "scripts": {
        "dev": "next src/",
        "build": "yarn generate:examples && yarn next build src/",
        "start": "next start src/",
        "serve": "cross-env NODE_ENV=production firebase serve",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "NODE_ENV=production yarn build && yarn firebase deploy --only functions,hosting",
        "logs": "firebase functions:log",
        "analyze": "ANALYZE=true next build src/",
        "lint": "eslint ./src",
        "generate:examples": "node ./scripts/generate-component-examples && yarn lint --fix"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@data-driven-forms/form-builder": "0.0.11-rc3",
        "@data-driven-forms/mui-component-mapper": "*",
        "@data-driven-forms/react-form-renderer": "*",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/styles": "^4.10.0",
        "@mdx-js/loader": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@next/bundle-analyzer": "^10.0.5",
        "@next/mdx": "^10.0.5",
        "@stackblitz/sdk": "^1.5.2",
        "clsx": "^1.1.1",
        "codesandbox": "2.2.1",
        "docsearch.js": "^2.6.3",
        "firebase-admin": "^8.12.1",
        "firebase-functions": "^3.6.1",
        "markdown-to-jsx": "^7.1.1",
        "next": "10.0.5",
        "next-offline": "^5.0.2",
        "prism-react-renderer": "^1.1.1",
        "prop-types": "^15.7.2",
        "raw-loader": "^4.0.2",
        "react": "^16.13.1",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "^16.13.1",
        "react-github-buttons": "^0.5.0",
        "react-sticky-box": "^0.9.3",
        "terser-webpack-plugin": "^4.2.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "firebase-functions-test": "^0.2.1",
        "firebase-tools": "^7.7.10",
        "rimraf": "^3.0.2"
    }
}
