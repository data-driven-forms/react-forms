{
  "name": "@data-driven-forms/react-form-renderer",
  "version": "1.0.0",
  "description": "React form renderer for data-driven-forms.",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "webpack-dev-server --env dev --config ./config/webpack.config.js --open --hot",
    "build": "webpack --env prod --config ./config/webpack.config.js",
    "test": "jest --coverage",
    "test-ci": "jest",
    "release": "semantic-release"
  },
  "repository": "git@github.com:data-driven-forms/react-forms.git",
  "keywords": [
    "react",
    "forms",
    "javascript"
  ],
  "jest": {
    "setupFiles": [
      "./config/jest.setup.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/.{snap}*",
      "!src/vendor.js"
    ],
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.x",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "semantic-release": "15.12.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "release": {
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json"
        ],
        "message": "Release of new version: ${nextRelease.version} <no> [skip ci]"
      }
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "final-form": "^4.12.0",
    "final-form-arrays": "^1.1.0",
    "react-final-form": "^4.1.0",
    "react-final-form-arrays": "2.0.1"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
