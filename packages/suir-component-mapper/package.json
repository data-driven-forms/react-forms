{
  "name": "@data-driven-forms/suir-component-mapper",
  "version": "2.4.8",
  "description": "Component mapper for Semantic ui react for data-driven-forms.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/cjs/index.d.ts",
  "files": [
    "dist/"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "start": "webpack-dev-server --env dev --config ./config/webpack.config.js --open --hot",
    "build": "yarn build:cjs && yarn build:esm && yarn build:umd && yarn build:typings",
    "build:cjs": "BABEL_ENV=cjs rollup -c ./rollup.config.js --format=cjs --environment FORMAT:cjs",
    "build:esm": "BABEL_ENV=esm rollup -c ./rollup.config.js --format=esm --environment FORMAT:esm",
    "build:umd": "rollup -c ./rollup.config.js --format=umd --environment FORMAT:umd",
    "build:typings": "node ../../scripts/copy-files.js",
    "release": "semantic-release"
  },
  "repository": "git@github.com:data-driven-forms/react-forms.git",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@data-driven-forms/react-form-renderer": "*",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-pluginutils": "^2.8.2",
    "sass-loader": "^7.1.0",
    "semantic-release": "^16.0.4",
    "semantic-ui-react": "^0.88.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "@data-driven-forms/react-form-renderer": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-react": "^0.88.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "prop-types": "^15.7.2",
    "react-jss": "^10.1.1"
  }
}
