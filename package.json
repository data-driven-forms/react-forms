      "react-dom": "^19.0.0"
    },
    "@fluentui/react-component-ref": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  },
  "dependencies": {
    "lz-string": "^1.5.0",
    "ts-node": "^10.9.1"
  }
}
